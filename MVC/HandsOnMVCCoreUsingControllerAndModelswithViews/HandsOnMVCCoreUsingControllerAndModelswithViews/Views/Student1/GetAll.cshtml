@using HandsOnMVCCoreUsingControllerAndModelswithViews.Models;
@{
	ViewData["Title"] = "GetAll";
	Layout = "~/Views/Shared/_Layout.cshtml";
	//List<Student> list = ViewData["list"] as List<Student>;
	List<Student> list = ViewBag.list;
}
<table class="table table-bordered">
	<tr>
		<th>ID</th>
		<th>Name</th>
		<th>Age</th>
		<th>Class</th>
	</tr>
	@foreach(var item in list)
	{
<tr>
	<td>@item.Sid</td>
	<td>@item.Sname</td>
	<td>@item.Age</td>
	<td>@item.Class</td>
</tr>
	}
</table>

